{
    "name": "major-phyo-san/prevent-same-shit",
    "description": "\"Prevent duplicate database records by calculating SHA256 hash for each database entry\"",
    "type": "library",
    "license": "MIT",
    "version": "0.0.19",
    "keywords": ["laravel", "hash", "duplicate-check", "row-hash", "package"],
    "homepage": "https://github.com/major-phyo-san/prevent-same-shit",
    "autoload": {
        "psr-4": {
            "MajorPhyoSan\\PreventSameShit\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Major Phyo San",
            "email": "phyojupiter9@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0 || ^8.1 || ^8.2 || ^8.3",
        "illuminate/database": "^8.0 || ^9.0 || ^10.0 || ^11.0",
        "illuminate/support": "^8.0 || ^9.0 || ^10.0 || ^11.0",
        "illuminate/console": "^8.0 || ^9.0 || ^10.0 || ^11.0",
        "illuminate/config": "^8.0 || ^9.0 || ^10.0 || ^11.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "MajorPhyoSan\\PreventSameShit\\PreventSameShitServiceProvider"
            ]
        }
    }
}
